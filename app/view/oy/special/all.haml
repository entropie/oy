#oy-page
  %h1#oy-topic All Pages
  #oy-all
    - i = 0
    - @contents.partition{|oby| (i+=1) % 2 == 0}.each do |row|
      %ul
        - row.each do |w|
          %li
            %a{:class => "oy-link x", :href => WikiController.r(w.link)}= w.title
            %span.oy-msg= " (#{w.message})"
            %span.oy-sha= " (#{w.commit.sha})"        

  
  
